{

  "name": "Check&Stock",
  "slug": "check_stock",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/images/icons/icone_base.png",
  "scheme": "checkstock",
  "userInterfaceStyle": "automatic",
  "newArchEnabled": true,
  "jsEngine": "hermes",
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/icons/icone_base.png",
      "backgroundColor": "#1E1E1E"
    },
    "edgeToEdgeEnabled": true,
    "package": "com.raphvssr.check_stock",
    "softwareKeyboardLayoutMode": "pan"
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/icons/icone_base.png",
        "backgroundColor": "#1E1E1E"
      }
    ],
    "expo-font",
    "expo-sqlite",
    "expo-web-browser"
  ],
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {},
    "eas": {
      "projectId": "64b9e450-db92-4e09-aa37-b76e9a10fa78"
    }
  },
  "owner": "raphvssr",
  "assetBundlePatterns": [
    "assets/**"
  ]
  
}
